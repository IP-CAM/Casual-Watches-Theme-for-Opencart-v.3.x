<nav class="links{% if class %} {{ class }}{% endif %}">
    <ul class="links__list">
        {% for link in links %}
            <li class="links__item">
                <a class="links__link" href="{{ link.href }}">
                    <span class="links__text">
                        {{ link.name }}
                    </span>
                </a>
            </li>
        {% endfor %}
    </ul>
</nav>
